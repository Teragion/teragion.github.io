                <span class="p">}</span>
            <span class="p">}</span>
            <span class="c1">// dp[i + s[j]] = max(dp[i + s[j]], dp[i] * ((double)p[j] / 100.0));</span>
        <span class="p">}</span>
    <span class="p">}</span>
